{
    "name": "1tomany/exif-tools",
    "description": "A simple, no dependency library that makes working with EXIF data easier",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "exif",
        "gps",
        "exif coordinates",
        "exif gps",
        "exif latitude",
        "exif longitude",
        "exif read",
        "exif reader"
    ],
    "require": {
        "php": ">=8.4",
        "ext-bcmath": "*",
        "ext-exif": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\ExifTools\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\ExifTools\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
